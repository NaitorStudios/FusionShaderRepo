<effect>
<name>Red Box</name>
<author>MuddyMole, DX11 Port by NaitorStudios</author>
<description>Renders a solid red box with adjustable green and blue color components.</description>
<parameter>
	<name>Green</name>
	<code>fGreen</code>
	<description>The green component value.</description>
	<type>int</type>
	<property>edit</property>
	<value>0</value>
	<min>0</min>
	<max>255</max>
</parameter>
<parameter>
	<name>Blue</name>
	<code>fBlue</code>
	<description>The blue component value.</description>
	<type>int</type>
	<property>edit</property>
	<value>0</value>
	<min>0</min>
	<max>255</max>
</parameter>

</effect>
