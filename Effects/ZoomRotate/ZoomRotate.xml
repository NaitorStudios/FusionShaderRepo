<effect>
<name>Zoom + Rotate</name>
<description>Zoom in and out + Rotate
&#xA;Last Updated: 2023-01-04 03:43</description>
<author>Sphax - Flavien Clermont</author>
<company>Complex-softwares, edit by NaitorStudios and Buttermun</company>
<website>http://complex.softwares.free.fr</website>
<parameter>
		<name>Width</name>
		<code>fWidth</code>
		<description>Width coefficient</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Height</name>
		<code>fHeight</code>
		<description>Height coefficient</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Zoom X</name>
		<code>fZoomX</code>
		<description>X center of the zoom</description>
		<type>float</type>
		<property>edit</property>
		<value>0.5</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Zoom Y</name>
		<code>fZoomY</code>
		<description>Y center of the zoom</description>
		<type>float</type>
		<property>edit</property>
		<value>0.5</value>
		<min>0.0</min>
</parameter>

<parameter>
		<name>Angle</name>
		<code>fAngle</code>
		<description>Angle degree</description>
		<type>float</type>
		<property>edit</property>
		<value>0.0</value>
</parameter>

<BackgroundTexture>1</BackgroundTexture>
<sampler>
<index>1</index>
<addressU>border</addressU>
<addressV>border</addressV>
<filter>linear</filter>
<BorderColor>-16777216</BorderColor>;
</sampler>
</effect>

