<effect>
<name>Shape Mask3</name>
<description>Mask Background using texture as alpha channel, this version repeats the mask texture...</description>
<author>mihailsh, DX11 port by NaitorStudios</author>
<parameter>
	<name>masktex</name>
	<code>masktex</code>
	<description>mask</description>
	<type>IMAGE</type>
	<property>image</property>
</parameter>

<parameter>
	<name>mask_x</name>
	<code>mask_x</code>
	<description>mask x coord</description>
	<type>int</type>
	<property>edit</property>
</parameter>

<parameter>
	<name>mask_y</name>
	<code>mask_y</code>
	<description>mask y coord</description>
	<type>int</type>
	<property>edit</property>
</parameter>

<parameter>
	<name>mask_width</name>
	<code>mask_width</code>
	<description>mask width</description>
	<type>int</type>
	<property>edit</property>
</parameter>

<parameter>
	<name>mask_height</name>
	<code>mask_height</code>
	<description>mask height</description>
	<type>int</type>
	<property>edit</property>
</parameter>

<sampler>
<index>1</index>
<filter>linear</filter>
<addressU>clamp</addressU>
<addressV>clamp</addressV>
</sampler>

</effect>

