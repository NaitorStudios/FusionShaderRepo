<effect>
<name>Starfield 8 Layer</name>
<description>Amiga Like 8 Layer StarField</description>
<author>Gigatron</author>
<parameter>
	<name>Timer</name>
	<description>Timer Value ; increase this value always set effect parameter 't_timer' to  Timer*0.002</description>
	<code>t_timer</code>
	<type>float</type>
	<value>0.002</value>
</parameter>
<parameter>
	<name>Speed</name>
	<description>Speed If Animated</description>
	<code>speed</code>
	<type>float</type>
	<value>0.05</value>
</parameter>

<parameter>
	<name>Direction</name>
	<description>define direction 0,1,2,3  up/down/left/right</description>
	<code>dir</code>
	<type>float</type>
	<value>1.0</value>
</parameter>
 </effect>

