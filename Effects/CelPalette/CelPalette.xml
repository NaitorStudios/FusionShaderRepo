<effect>
<name>CelPalette</name>
<author></author>
<description>Apply a normal map to an object
&#xA;Last Updated: 2026-01-11 00:54</description>

<!--TEXTURE-->

	<parameter>
		<name>Normal Map</name>
		<code>normalMap</code>
		<description></description>
		<type>image</type>
		<property>image</property>
	</parameter>

	<parameter>
		<name>diffuseMap</name>
		<code>diffuseMap</code>
		<description></description>
		<type>image</type>
		<property>image</property>
	</parameter>
	
	<parameter>
		<name>Object X</name>
		<code>objX</code>
		<description>X Position of the object</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
	</parameter>
	<parameter>
		<name>Object Y</name>
		<code>objY</code>
		<description>Y Position of the object</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
	</parameter>


<!--NORMALS-->

	<!--LIGHT 1-->


	<parameter>
		<name>Line color2</name>
		<code>ambientLightColor</code>
		<description>Ths color will show wherever the alpha of the Normal Map is 0 while the alpha on the texture isn't.</description>
		<type>INT_FLOAT4</type>
		<property>color</property>
		<value>0</value>
	</parameter>
	<parameter>
		<name>Line color</name>
		<code>directLightColor</code>
		<description>Ths color will show wherever the alpha of the Normal Map is 0 while the alpha on the texture isn't.</description>
		<type>INT_FLOAT4</type>
		<property>color</property>
		<value>0</value>
	</parameter>

	
	<parameter>
		<name>Light 1 Pos X</name>
		<code>lightX</code>
		<description>X position of light 1</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
	</parameter>	
	<parameter>
		<name>Light 1 Pos Y</name>
		<code>lightY</code>
		<description>Y position of light 1</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
	</parameter>
	<parameter>
		<name>Light 1 Pos Z</name>
		<code>lightZ</code>
		<description>Z position of light 1</description>
		<type>float</type>
		<property>edit</property>
		<value>3.0</value>
	</parameter>		
	

</effect>
