<effect>
<name>Crop with Smooth Edges</name>
<description>Crop the image with smooth edges and set the transparency of the cropped surface</description>
<author>Sphax - Flavien Clermont, Edit by NaitorStudios</author>
<company>Complex-softwares</company>
<website>http://complex.softwares.free.fr</website>
<parameter>
        <name>Left</name>
        <code>fL</code>
        <description>Left border width</description>
        <type>float</type>
        <property>edit</property>
        <value>0.0</value>
        <min>0.0</min>
        <max>1.0</max>
</parameter>
<parameter>
        <name>Right</name>
        <code>fR</code>
        <description>Right border width</description>
        <type>float</type>
        <property>edit</property>
        <value>1.0</value>
        <min>0.0</min>
        <max>1.0</max>
</parameter>
<parameter>
        <name>Top</name>
        <code>fT</code>
        <description>Top border width</description>
        <type>float</type>
        <property>edit</property>
        <value>0.0</value>
        <min>0.0</min>
        <max>1.0</max>
</parameter>
<parameter>
        <name>Bottom</name>
        <code>fB</code>
        <description>Bottom border width</description>
        <type>float</type>
        <property>edit</property>
        <value>1.0</value>
        <min>0.0</min>
        <max>1.0</max>
</parameter>
<parameter>
        <name>Left Smoothness</name>
        <code>fLSmooth</code>
        <description>Left border smoothness</description>
        <type>float</type>
        <property>edit</property>
        <value>0.1</value>
        <min>0.0</min>
        <max>0.5</max>
</parameter>
<parameter>
        <name>Right Smoothness</name>
        <code>fRSmooth</code>
        <description>Right border smoothness</description>
        <type>float</type>
        <property>edit</property>
        <value>0.1</value>
        <min>0.0</min>
        <max>0.5</max>
</parameter>
<parameter>
        <name>Top Smoothness</name>
        <code>fTSmooth</code>
        <description>Top border smoothness</description>
        <type>float</type>
        <property>edit</property>
        <value>0.1</value>
        <min>0.0</min>
        <max>0.5</max>
</parameter>
<parameter>
        <name>Bottom Smoothness</name>
        <code>fBSmooth</code>
        <description>Bottom border smoothness</description>
        <type>float</type>
        <property>edit</property>
        <value>0.1</value>
        <min>0.0</min>
        <max>0.5</max>
</parameter>
<parameter>
        <name>Alpha</name>
        <code>fA</code>
        <description>Transparency of the cropped surface</description>
        <type>float</type>
        <property>edit</property>
        <value>1.0</value>
        <min>0.0</min>
        <max>1.0</max>
</parameter>
</effect>