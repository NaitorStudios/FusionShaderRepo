<effect>
<name>Zoom</name>
<description>Zoom in and out
&#xA;Last Updated: 2026-01-11 00:54</description>
<author>Sphax - Flavien Clermont, Improved by NaitorStudios</author>
<company>Complex-softwares</company>
<website>http://complex.softwares.free.fr</website>

<sampler>
	<index>0</index>
	<filter>linear</filter>
	<AddressU>border</AddressU>
	<AddressV>border</AddressV>
	<BorderColor>16777215</BorderColor>
</sampler>

<parameter>
		<name>Width</name>
		<code>fWidth</code>
		<description>Width coefficient</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Height</name>
		<code>fHeight</code>
		<description>Height coefficient</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Zoom X</name>
		<code>fZoomX</code>
		<description>X center of the zoom</description>
		<type>float</type>
		<property>edit</property>
		<value>0.5</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Zoom Y</name>
		<code>fZoomY</code>
		<description>Y center of the zoom</description>
		<type>float</type>
		<property>edit</property>
		<value>0.5</value>
		<min>0.0</min>
</parameter>

</effect>

